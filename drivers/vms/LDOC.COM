$! LDOC.COM - Command file to load VSTADOC
$!
$WRITE SYS$OUTPUT "[Creating VSTADOC.EXE]"
$LINK /EXEC=VSTADOC -
	LDOC.OPT/OPTION, -
	VT100, -
	SYS$LIBRARY:RECOG/LIB
$!
$! And a debugging version..
$!
$WRITE SYS$OUTPUT "[Creating VSTADOCD.EXE]"
$LINK /DEBUG/EXEC=VSTADOCD -
	LDOC.OPT/OPTION, -
	VT100, -
	SYS$LIBRARY:RECOG/LIB
$! [END OF LDOC.COM]
