%REPLACE VSTA$S_VKB_MODE_BITS BY 2;
DCL 1 VKB_MODE_BITS  BASED ,
     2 VSTA$V_RESERVED bit(6),          /*RESERVED VKB STATE BITS           */
     2 VSTA$V_LEVEL bit(4),             /*VKB LEVEL 1/2 CONFORMANCE         */
     2 VSTA$V_EIGHTBIT bit(1),          /*VKB NATIVE EIGHT-BIT/SEVEN-BIT    */
     2 VSTA$V_KEYPAD bit(1),            /*VKB KEYPAD APPLICATION MODE       */
     2 VSTA$V_CURSOR_KEY bit(1),        /*VKB CURSOR KEY MODE               */
     2 VSTA$V_VT52 bit(1),              /*VKB VT52 MODE                     */
     2 VSTA$V_NEW_LINE bit(1),          /*VKB NEW-LINE MODE                 */
     2 VSTA$V_EXTRA bit(1);             /*FOR FUTURE DEC ABUSE              */
