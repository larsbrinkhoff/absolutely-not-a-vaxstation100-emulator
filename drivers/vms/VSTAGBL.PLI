/*                                                                          */
/* RENDITION SYMBOLS                                                        */
/*                                                                          */
%REPLACE VSTA$M_REND_BOLD BY '10000000000000000000000000000000'b;
%REPLACE VSTA$M_REND_ITALICS BY '01000000000000000000000000000000'b;
%REPLACE VSTA$M_REND_UNDERSCORE BY '00100000000000000000000000000000'b;
%REPLACE VSTA$M_REND_BLINK BY '00010000000000000000000000000000'b;
%REPLACE VSTA$M_REND_REVERSE BY '00001000000000000000000000000000'b;
%REPLACE VSTA$M_REND_EXTRA BY '00000111000000000000000000000000'b;
%REPLACE VSTA$K_REND_BLOCK_SIZE BY 1;   /*LENGTH OF DATA BLOCK              */
 
%REPLACE VSTA$S_REND_BITS BY 1;
DCL 1 REND_BITS  BASED ,
     2 VSTA$V_REND_BOLD bit(1),         /*BOLD RENDITION                    */
     2 VSTA$V_REND_ITALICS bit(1),      /*ITALICS RENDITION                 */
     2 VSTA$V_REND_UNDERSCORE bit(1),   /*UNDERSCORE RENDITION              */
     2 VSTA$V_REND_BLINK bit(1),        /*BLINK RENDITION                   */
     2 VSTA$V_REND_REVERSE bit(1),      /*REVERSE VIDEO RENDITION           */
     2 VSTA$V_REND_EXTRA bit(3);        /*FOR FUTURE DEC ABUSE              */
