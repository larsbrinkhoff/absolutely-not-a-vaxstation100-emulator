MODULE vsast;
/*
/* Definitions used by AST handling in the ACP.
/*

/*
/* gnq_block is the queue entry for generic VD actions.
/* (like "create job running on a VT100"), "delete job", etc.
/*
AGGREGATE gnq_block STRUCTURE PREFIX gnq$;
	flink		ADDRESS;
	blink		ADDRESS;
	function	WORD;		/* (not needed??)
	filler		WORD;		/* to force longword boundary
	routine		ADDRESS;	/* Addr of routine to call
	param		LONGWORD;	/* longword passed to routine
END gnq_block;

END_MODULE;
